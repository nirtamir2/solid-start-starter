{
  "name": "solid-start-starter",
  "version": "0.0.0",
  "private": false,
  "description": "SolidJS project starter template with TypeScript, ESLint, TailwindCSS, tRPC and Prisma",
  "keywords": [
    "solid",
    "template",
    "solid-start",
    "solidjs"
  ],
  "homepage": "https://github.com/nirtamir2/solid-start-starter#readme",
  "bugs": {
    "url": "https://github.com/nirtamir2/solid-start-starter/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nirtamir2/solid-start-starter.git"
  },
  "license": "MIT",
  "author": "nirtamir2",
  "type": "module",
  "scripts": {
    "build": "solid-start build && pnpm run postbuild",
    "postbuild": "cp node_modules/@prisma/engines/*query* .vercel/output/functions/render.func/ && cp prisma/schema.prisma .vercel/output/functions/render.func/",
    "dev": "solid-start dev",
    "format": "prettier \"**/*\" --write --ignore-unknown",
    "postinstall": "prisma generate && node add-ts-nocheck.cjs",
    "lint": "eslint --max-warnings 0 --fix \"**/*.{ts,tsx,js,jsx}\"",
    "prepare": "husky install",
    "prisma-push": "prisma db push",
    "start": "solid-start start",
    "type-check": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{ts,tsx,md}": "eslint --cache --fix",
    "*.{ts,tsx,css,html,md}": "prettier --write"
  },
  "dependencies": {
    "@motionone/solid": "^10.14.3",
    "@prisma/client": "^4.6.1",
    "@solid-auth/core": "^0.0.1",
    "@solid-auth/socials": "^0.0.2",
    "@solid-primitives/i18n": "^1.1.2",
    "@solidjs/meta": "^0.28.0",
    "@solidjs/router": "^0.5.1",
    "@trpc/client": "10.2.0",
    "@trpc/server": "10.2.0",
    "@upstash/ratelimit": "^0.1.5",
    "@upstash/redis": "^1.18.0",
    "dayjs": "^1.11.6",
    "dotenv": "^16.0.3",
    "solid-devtools": "^0.23.1",
    "solid-js": "^1.6.2",
    "solid-start": "^0.2.1",
    "solid-start-trpc": "^0.0.13",
    "undici": "5.13.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@tsconfig/strictest": "^1.0.2",
    "@types/node": "^16.18.3",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "eslint-config-nirtamir2": "^0.0.44",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "prettier-plugin-packagejson": "^2.3.0",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "prisma": "^4.6.1",
    "solid-start-node": "^0.2.1",
    "solid-start-vercel": "^0.2.5",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-solid-svg": "^0.6.0",
    "vitest": "^0.25.3"
  },
  "packageManager": "pnpm@7.5.0",
  "engines": {
    "node": ">=16"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm ./prisma/seed.ts"
  }
}
