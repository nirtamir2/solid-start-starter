{
  "name": "solid-start-starter",
  "type": "module",
  "scripts": {
    "build": "solid-start build && pnpm run postbuild",
    "postbuild": "cp node_modules/@prisma/engines/*query* .vercel/output/functions/render.func/ && cp prisma/schema.prisma .vercel/output/functions/render.func/",
    "dev": "solid-start dev",
    "format": "prettier \"**/*\" --write --ignore-unknown",
    "postinstall": "prisma generate && node add-ts-nocheck.cjs",
    "lint": "eslint --fix \"**/*.{ts,tsx,js,jsx}\"",
    "prepare": "husky install",
    "prisma-push": "prisma db push",
    "start": "solid-start start",
    "type-check": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{ts,tsx,md}": "eslint --cache --fix",
    "*.{ts,tsx,css,html,md}": "prettier --write"
  },
  "dependencies": {
    "@prisma/client": "^4.6.1",
    "@solidjs/meta": "^0.28.0",
    "@solidjs/router": "^0.5.0",
    "@trpc/client": "10.0.0-rc.7",
    "@trpc/server": "10.0.0-rc.7",
    "@upstash/ratelimit": "^0.1.5",
    "@upstash/redis": "^1.16.0",
    "dotenv": "^16.0.3",
    "solid-devtools": "^0.21.0",
    "solid-js": "^1.6.2",
    "solid-start": "^0.2.1",
    "solid-start-trpc": "^0.0.12",
    "undici": "5.12.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@tsconfig/strictest": "^1.0.2",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.27.0",
    "eslint-config-nirtamir2": "^0.0.41",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "prettier-plugin-packagejson": "^2.3.0",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prisma": "^4.6.1",
    "solid-start-node": "^0.2.1",
    "solid-start-vercel": "^0.2.5",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.3",
    "vite": "^3.2.3"
  },
  "engines": {
    "node": ">=16"
  }
}
